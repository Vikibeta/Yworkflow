<!-- start 完本精品 -->
<div class="finish-book-wrap box-center mb40 cf">
    <div class="left-wrap fl" data-aid="qd_A127">
        <h3 class="wrap-title">
            完本精品<span>完本总数<%=  finBookRecom['finBookCnt'] %>本</span><a class="more" href="http://quanben.qidian.com/Default.aspx" target="_blank" data-aid="qd_A128"><em>
            &#124;</em>更多</a></h3>
        <div class="left-info fl">
            <div class="slide-box">
                <div class="la-ball-pulse">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
                <ul id="left-slide-02" class="roundabout">
                    <% for(var i=0; i < finBookRecom['finBkFocus'].length; i++) { %>
                    <li class="book<%=i+1%>" data-id="<%=i+1%>" data-type="1" data-width="93px" data-height="100%">
                        <img class="lazy" data-original="<%= finBookRecom['finBkFocus'][i]['imageUrl']%>">
                    </li>
                    <% } %>
                </ul>
            </div>
            <div class="info-text">
                <dl>
                    <dd>
                        <h3>
                            <a href="http://www.qidian.com/Book/<%= finBookRecom['finBkFocus'][0]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= finBookRecom['finBkFocus'][0]['bookId'] %>"><%=
                                finBookRecom['finBkFocus'][0]['bookName']%></a></h3>
                        <p class="author">
                            <a href="http://me.qidian.com/authorIndex.aspx?id=<%= finBookRecom['finBkFocus'][0]['authorId'] %>"><%=
                                finBookRecom['finBkFocus'][0]['wordsCount']%>字</a><em>&#183;</em><%=
                            finBookRecom['finBkFocus'][0]['praiseCnt'] %>分</p>
                        <p class="total"><b><%= finBookRecom['finBkFocus'][0]['likeCnt'] %></b>人在追</p>
                        <p class="intro">
                            <%= finBookRecom['finBkFocus'][0]['recomWords'] %>
                        </p>
                        <a class="blue-btn" href="http://www.qidian.com/Book/<%= finBookRecom['finBkFocus'][0]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= finBookRecom['finBkFocus'][0]['bookId'] %>">立即阅读</a>
                    </dd>
                    <% for(var i=1; i < finBookRecom['finBkFocus'].length; i++) { %>
                    <dd class="hidden">
                        <h3>
                            <a href="http://www.qidian.com/Book/<%= finBookRecom['finBkFocus'][i]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= finBookRecom['finBkFocus'][i]['bookId'] %>"><%=
                                finBookRecom['finBkFocus'][i]['bookName']%></a></h3>
                        <p class="author">
                            <a href="http://me.qidian.com/authorIndex.aspx?id=<%= finBookRecom['finBkFocus'][i]['authorId'] %>"><%=
                                finBookRecom['finBkFocus'][i]['wordsCount']%>字</a><em>&#183;</em><%=
                            finBookRecom['finBkFocus'][i]['praiseCnt'] %>分</p>
                        <p class="total"><b><%= finBookRecom['finBkFocus'][i]['likeCnt'] %></b>人在追</p>
                        <p class="intro">
                            <%= finBookRecom['finBkFocus'][i]['recomWords'] %>
                        </p>
                        <a class="blue-btn" href="http://www.qidian.com/Book/<%= finBookRecom['finBkFocus'][i]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= finBookRecom['finBkFocus'][i]['bookId'] %>">立即阅读</a>
                    </dd>
                    <% } %>
                </dl>
            </div>
        </div>
        <div class="center-book-list fl">
            <div class="line line1"></div>
            <div class="line line2"></div>
            <ul>
                <% for(var i=0; i < recomFinBook.length; i++) { %>
                <li>
                    <div class="book-img">
                        <a href="http://www.qidian.com/Book/<%= recomFinBook[i]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= recomFinBook[i]['bookId'] %>"><img class="lazy" data-original="<%=  recomFinBook[i]['imageUrl'] %>" alt="">
                            <%
                                //评分小于7.0时不显示
                                var score = recomFinBook[i]['praiseCnt']
                                if (score < 7.0) {
                                    var hidden="class=hidden"
                                }
                            %>
                            <span <%=hidden%>><%= score %></span>
                        </a>
                    </div>
                    <div class="book-info">
                        <h3>
                            <a href="http://www.qidian.com/Book/<%= recomFinBook[i]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= recomFinBook[i]['bookId'] %>"><%=
                                recomFinBook[i]['recomTitle'] %>
                            </a></h3>
                        <p><%= recomFinBook[i]['recomWords'] %></p>
                        <div class="state-box cf">
                            <span><%=  recomFinBook[i]['wordsCount'] %>字</span><i><%= recomFinBook[i]['subCategoryName']
                            %></i><a class="author" href="http://me.qidian.com/authorIndex.aspx?id=<%= recomFinBook[i]['authorId']  %>" target="_blank"><img class="lazy" data-original="<%= staticConf[envType].staticPath %>/images/ico/user.png" alt=""><%=
                            recomFinBook[i]['authorName'] %></a>
                        </div>
                    </div>
                </li>
                <% } %>
            </ul>
        </div>
    </div>
    <!-- start 最近完本-->
    <div class="right-wrap recent-finish-wrap fr" data-aid="qd_A129">
        <div class="score-list">
            <h3 class="wrap-title">最近完本</h3>
            <div class="book-list">
                <ul>
                    <li class="unfold">
                        <div class="book-wrap cf">
                            <div class="book-info fl">
                                <h3>玄幻</h3>
                                <h4>
                                    <a href="http://www.qidian.com/Book/<%= chartLastFinBook[0]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= chartLastFinBook[0]['bookId'] %>"><%=
                                        chartLastFinBook[0]['bookName'] %></a></h4>
                                <p class="score"><em><%= chartLastFinBook[0]['praiseCnt'] %></em>分<cite>&#183;</cite><%=
                                    chartLastFinBook[0]['wordsCount'] %>字</p>
                                <p class="author">
                                    <a href="http://me.qidian.com/authorIndex.aspx?id=<%= chartLastFinBook[0].authorId %>" target="_blank"><%=
                                        chartLastFinBook[0]['authorName'] %></a></p>
                            </div>
                            <div class="book-cover">
                                <a class="link" href="http://www.qidian.com/Book/<%= chartLastFinBook[0]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= chartLastFinBook[0]['bookId'] %>">
                                    <img src="<%= chartLastFinBook[0]['imageUrl']  %>" alt="">
                                </a>
                                <span></span>
                            </div>
                        </div>
                    </li>
                    <% for(var i=1; i < chartLastFinBook.length; i++) { %>
                    <li>
                    <a class="name" href="http://www.qidian.com/Book/<%= chartLastFinBook[i]['bookId'] %>.aspx %>" target="_blank" data-bid="<%= chartLastFinBook[i]['bookId'] %>"><%= chartLastFinBook[i]['bookName'] %></a><i><%= chartLastFinBook[i]['wordsCount'] %>字</i>
                    </li>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
    <!-- end 最近完本-->
</div>
<!-- end 完本精品 -->